<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane id="BorderPane" fx:id="root" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefHeight="100000.0" prefWidth="100000.0" rotate="0.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.github.orekyuu.javatterfx.controller.JavatterTimeLineController">
  <center>
    <ScrollPane fx:id="scroll" fitToHeight="false" fitToWidth="true" hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefHeight="-1.0" prefWidth="-1.0" vbarPolicy="ALWAYS">
      <content>
        <VBox fx:id="box" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefHeight="-1.0" prefWidth="-1.0" />
      </content>
    </ScrollPane>
  </center>
  <rotationAxis>
    <Point3D y="1.0" />
  </rotationAxis>
  <top>
    <ToolBar fx:id="bar" maxWidth="1.7976931348623157E308" minWidth="0.0">
      <contextMenu>
        <ContextMenu>
          <items>
            <MenuItem mnemonicParsing="false" onAction="#onScrollTop" text="最新へ移動" />
            <MenuItem mnemonicParsing="false" onAction="#onClose" text="閉じる" />
          </items>
        </ContextMenu>
      </contextMenu>
      <effect>
        <InnerShadow />
      </effect>
      <items>
        <Label fx:id="name" text="Name" />
      </items>
    </ToolBar>
  </top>
</BorderPane>
